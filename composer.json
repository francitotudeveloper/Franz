{
    "name": "franz/parcial1",
    "description": "Libreria de adm. de usuarios",
    "keywords": ["parcial1", "roles", "permissions", "auth", "laravel", "franz"],
    
    "authors": [{
        "name": "franz suarez",
        "email": "fcsg1997@gmail.com"
    }],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/filesystem": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/config": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Franz\\Parcial1\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Franz\\Parcial1\\Parcial1ServiceProvider"
            ],
            "aliases": {
                "Parcial1": "Franz\\Parcial1\\Facades\\Parcial1"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^6.4",
       
        "orchestra/testbench": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Franz\\Parcial1\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "vendor/bin/phpcs src/*"
    }
}
